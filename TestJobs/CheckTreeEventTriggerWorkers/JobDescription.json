{
  "JobDescription": {
    "LogicalName": "Check TreeEventTrigger Workers",
    "LogicalExpression": "Datum & @Bool.true & flipflop",
    "JobSnapshotTrigger": {
      "Reference": "AnyLogicalResultChanged|Exception",
      "Parameters": "Check TreeEventTrigger Workers"
    },
    "BreakWithResult": false,
    "ThreadLocked": false,
    "StartCollapsed": false,
    "IsVolatile": false,
    "Triggers": {
      "Trigger": [
        {
          "LogicalName": "FirstNamedTimerTrigger",
          "PhysicalPath": "TimerTrigger.dll",
          "Parameters": "S:5|S:5"
        }
      ]
    },
    "Loggers": {
      "Logger": [
        {
          "LogicalName": "FirstNamedLogger",
          "PhysicalPath": "TextFileLogger.dll",
          "Parameters": "Exception|LogicalResultChanged"
        }
      ]
    },
    "JobLogger": {
      "Reference": "FirstNamedLogger"
    },
    "Checkers": {
      "Checker": [
        {
          "LogicalName": "Datum",
          "PhysicalPath": "CheckDate.dll",
          "Trigger": {
            "Reference": "LogicalResultChanged|Exception",
            "Parameters": "Check TreeEventTrigger Workers"
          }
        },
        {
          "LogicalName": "flipflop",
          "PhysicalPath": "TrueFalseExceptionChecker.dll",
          "Parameters": "Null:True:False:Exception",
          "Trigger": {
            "Reference": "FirstNamedTimerTrigger"
          }
        }
      ]
    },
    "Workers": {
      "Worker": [
        {
          "LogicalExpression": "flipflop:False|Exception",
          "SubWorkers": {
            "SubWorker": [
              {
                "PhysicalPath": "VishnuMessageBox.exe",
                "Parameters": "-Message=\"%Timestamp%: %MachineName% %Event% von %Source% in %Sender%#Logical: %Logical% %Exception%\" -Caption=\"Ereignis Information\" -MessageNewLine=#"
              }
            ]
          }
        }
      ]
    }
  }
}