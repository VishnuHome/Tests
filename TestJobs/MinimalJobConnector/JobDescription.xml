<?xml version="1.0" encoding="utf-8"?>
<JobDescription>
  <LogicalName>Minimal Job Connector</LogicalName>
  <LogicalExpression>
    <![CDATA[SubJob1 AND JobConnector1]]>
  </LogicalExpression>
  <Loggers type="array">
    <Logger>
      <LogicalName>FirstNamedLogger</LogicalName>
      <PhysicalPath>TextFileLogger.dll</PhysicalPath>
      <Parameters>LogicalResultChanged|Exception</Parameters>
    </Logger>
  </Loggers>
  <Checkers type="Array">
    <Checker>
      <LogicalName>JobConnector1</LogicalName>
      <PhysicalPath>TriggerEventMirrorChecker.dll</PhysicalPath>
      <Parameters></Parameters>
      <Trigger>
        <LogicalName>AnyName</LogicalName>
        <Reference>LogicalResultChanged|Exception</Reference>
        <Parameters>SubJob1</Parameters>
      </Trigger>
      <Logger>
        <Reference>FirstNamedLogger</Reference>
      </Logger>
    </Checker>
  </Checkers>
  <SubJobs>
    <SubJob>
      <LogicalName>SubJob1</LogicalName>
      <LogicalExpression><![CDATA[IS SingleNode1]]></LogicalExpression>
    	<Checkers>
        <Checker>
          <LogicalName>SingleNode1</LogicalName>
          <PhysicalPath>TrueFalseExceptionChecker.dll</PhysicalPath>
          <Parameters>True:False|5|SingleNode1-Result</Parameters>
          <Trigger>
            <PhysicalPath>TimerTrigger.dll</PhysicalPath>
            <Parameters>S:10</Parameters>
          </Trigger>
        </Checker>
      </Checkers>
    </SubJob>
  </SubJobs>
</JobDescription>
