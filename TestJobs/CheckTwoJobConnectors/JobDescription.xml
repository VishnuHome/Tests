<?xml version="1.0" encoding="utf-8"?>
<JobDescription>
  <LogicalName>JobConnectorTest</LogicalName>
  <LogicalExpression>
    <![CDATA[SubJob1 AND SubJob2 AND SubJob3]]>
  </LogicalExpression>
  <JobLogger>
    <PhysicalPath>TextFileLogger.dll</PhysicalPath>
    <Parameters>AnyLogicalResultChanged|Exception</Parameters>
  </JobLogger>
  <Loggers type="array">
    <Logger>
      <LogicalName>FirstNamedLogger</LogicalName>
      <PhysicalPath>TextFileLogger.dll</PhysicalPath>
      <Parameters>Exception</Parameters>
    </Logger>
  </Loggers>
  <SubJobs>
    <SubJob>
      <LogicalName>SubJob1</LogicalName>
      <LogicalExpression><![CDATA[IS Checker]]></LogicalExpression>
      <Checkers type="Array">
	      <!--Checker>
	        <LogicalName>Checker</LogicalName>
	        <PhysicalPath>WPFDialogChecker.dll</PhysicalPath>
	        <Parameters>Checker-Result</Parameters>
	      </Checker-->
		    <Checker>
		      <LogicalName>Checker</LogicalName>
		      <PhysicalPath>TrueFalseExceptionChecker.dll</PhysicalPath>
		      <Parameters>False:Exception:True|2</Parameters>
		      <Trigger>
		        <PhysicalPath>TimerTrigger.dll</PhysicalPath>
		        <Parameters>S:2</Parameters>
		      </Trigger>
		    </Checker>
      </Checkers>
    </SubJob>
    <SubJob>
      <LogicalName>SubJob2</LogicalName>
      <LogicalExpression><![CDATA[SubJob1 & SubJob1Value1 & SubJob1Value2]]></LogicalExpression>
      <Checkers type="Array">
        <Checker>
          <LogicalName>SubJob1Value1</LogicalName>
          <PhysicalPath>TriggerEventMirrorChecker.dll</PhysicalPath>
          <Parameters>IsMirror</Parameters>
          <Trigger>
            <LogicalName>FirstNamedTreeEventTrigger</LogicalName>
            <Reference>LogicalResultChanged|Exception</Reference>
            <Parameters>SubJob1</Parameters>
          </Trigger>
          <Logger>
            <Reference>FirstNamedLogger</Reference>
          </Logger>
        </Checker>
        <Checker>
          <LogicalName>SubJob1Value2</LogicalName>
          <PhysicalPath>TriggerEventMirrorChecker.dll</PhysicalPath>
          <Parameters>IsMirror</Parameters>
          <Trigger>
            <LogicalName>FirstNamedTreeEventTrigger</LogicalName>
            <Reference>LogicalResultChanged|Exception</Reference>
            <Parameters>SubJob1</Parameters>
          </Trigger>
          <Logger>
            <Reference>FirstNamedLogger</Reference>
          </Logger>
        </Checker>
      </Checkers>
    </SubJob>
    <SubJob>
      <LogicalName>SubJob3</LogicalName>
      <LogicalExpression><![CDATA[SubJob1 & SubJob1Value3 & SubJob1Value4]]></LogicalExpression>
      <Checkers type="Array">
        <Checker>
          <LogicalName>SubJob1Value3</LogicalName>
          <PhysicalPath>TriggerEventMirrorChecker.dll</PhysicalPath>
          <Parameters>IsMirror</Parameters>
          <Trigger>
            <LogicalName>FirstNamedTreeEventTrigger</LogicalName>
            <Reference>LogicalResultChanged|Exception</Reference>
            <Parameters>SubJob1</Parameters>
          </Trigger>
          <Logger>
            <Reference>FirstNamedLogger</Reference>
          </Logger>
        </Checker>
        <Checker>
          <LogicalName>SubJob1Value4</LogicalName>
          <PhysicalPath>TriggerEventMirrorChecker.dll</PhysicalPath>
          <Parameters>IsMirror</Parameters>
          <Trigger>
            <LogicalName>FirstNamedTreeEventTrigger</LogicalName>
            <Reference>LogicalResultChanged|Exception</Reference>
            <Parameters>SubJob1</Parameters>
          </Trigger>
          <Logger>
            <Reference>FirstNamedLogger</Reference>
          </Logger>
        </Checker>
      </Checkers>
    </SubJob>
  </SubJobs>
</JobDescription>
