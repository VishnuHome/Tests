{
  "JobDescription": {
    "LogicalName": "TwoDialogNodes",
    "LogicalExpression": "(Node1) & (Node2)",
    "Checkers": {
      "Checker": [
        {
          "LogicalName": "Node1",
          "PhysicalPath": "WPFDialogChecker.dll",
          "Parameters": "Checker-Result"
        },
        {
          "LogicalName": "Node2",
          "PhysicalPath": "WPFDialogChecker.dll",
          "Parameters": "Checker-Result"
        }
      ]
    },
    "Workers": {
      "Worker": [
        {
          "LogicalExpression": "Node1:Exception",
          "SubWorkers": {
            "SubWorker": [
              {
                "PhysicalPath": "VishnuMessageBox.exe",
                "Parameters": "-Message=\"%Timestamp%: %Event% von %Source% in %Sender%#%Exception%\" -Caption=\"geplante Exception\" -MessageNewLine=#"
              }
            ]
          }
        },
        {
          "LogicalExpression": "Node2:Exception",
          "SubWorkers": {
            "SubWorker": [
              {
                "PhysicalPath": "VishnuMessageBox.exe",
                "Parameters": "-Message=\"%Timestamp%: %Event% von %Source% in %Sender%#%Exception%\" -Caption=\"geplante Exception\" -MessageNewLine=#"
              }
            ]
          }
        }
      ]
    }
  }
}