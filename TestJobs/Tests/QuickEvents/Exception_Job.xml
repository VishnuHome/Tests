<?xml version="1.0" encoding="utf-8"?>
<JobDescription>
  <LogicalName>Exception_Job</LogicalName>
  <LogicalExpression>
    <![CDATA[IS ExceptionJob]]>
  </LogicalExpression>
  <BreakWithResult>false</BreakWithResult>
  <ThreadLocked>false</ThreadLocked>
  <StartCollapsed>false</StartCollapsed>
  <JobLogger>
    <Reference>FirstNamedLogger</Reference>
  </JobLogger>
  <Checkers type="array">
    <Checker>
      <LogicalName>ExceptionJob</LogicalName>
      <PhysicalPath>ExceptionChecker.dll</PhysicalPath>
      <Parameters>4</Parameters>
      <ThreadLocked>true</ThreadLocked>
      <Trigger>
        <Reference>FirstNamedTimerTrigger</Reference>
      </Trigger>
    </Checker>
  </Checkers>
  <Workers type="array">
    <Worker>
      <LogicalExpression>Exception_Job:Exception</LogicalExpression>
      <SubWorkers type="array">
        <SubWorker>
          <PhysicalPath>ConsoleMessageBox.exe</PhysicalPath>
          <Parameters>%Timestamp%: Exception_Job %Event% von %Source% in %Sender%|%Exception%|Exception_Job Fehler</Parameters>
          <!-- Trigger>
            <LogicalName></LogicalName>
            <PhysicalPath>TimerTrigger.dll</PhysicalPath>
            <Parameters>S:8|S:1</Parameters>
          </Trigger -->
        </SubWorker>
      </SubWorkers>
    </Worker>
    <Worker>
      <LogicalExpression>Exception_Job:False</LogicalExpression>
      <SubWorkers type="array">
        <SubWorker>
          <PhysicalPath>ConsoleMessageBox.exe</PhysicalPath>
          <Parameters>%Timestamp%: Der Exception_Job hat scheinbar False zurückgegeben!|Vishnu Fehler</Parameters>
          <!-- Trigger>
            <LogicalName></LogicalName>
            <PhysicalPath>TimerTrigger.dll</PhysicalPath>
            <Parameters>S:8|S:1</Parameters>
          </Trigger -->
        </SubWorker>
      </SubWorkers>
    </Worker>
  </Workers>
</JobDescription>
