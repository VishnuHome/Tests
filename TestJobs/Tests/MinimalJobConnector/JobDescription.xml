<?xml version="1.0" encoding="utf-8"?>
<JobDescription>
  <LogicalName>JobConnectorTest</LogicalName>
  <LogicalExpression>
    <![CDATA[SubJob1 AND SubJob2]]>
  </LogicalExpression>
  <JobSnapshotTrigger>
    <Reference>AnyLogicalResultChanged|Exception</Reference>
    <Parameters>JobConnectorTest</Parameters>
  </JobSnapshotTrigger>
  <Loggers type="array">
    <Logger>
      <LogicalName>FirstNamedLogger</LogicalName>
      <PhysicalPath>TextFileLogger.dll</PhysicalPath>
      <Parameters>LogicalResultChanged|Exception</Parameters>
    </Logger>
  </Loggers>
  <SubJobs>
    <SubJob>
      <LogicalName>SubJob1</LogicalName>
      <LogicalExpression><![CDATA[:SingleNode1]]></LogicalExpression>
      <!--JobLogger>
      	<Reference>FirstNamedLogger</Reference>
      </JobLogger-->
      <Checkers type="Array">
        <!--Checker>
          <LogicalName>SingleNode1</LogicalName>
          <PhysicalPath>WPFDialogChecker.dll</PhysicalPath>
          <Parameters>DialogChecker-Result</Parameters>
        </Checker-->
        <Checker>
          <LogicalName>SingleNode1</LogicalName>
          <PhysicalPath>TrueFalseExceptionChecker.dll</PhysicalPath>
          <!--Parameters>Exception:True:Null:False|5|SingleNode1-Result</Parameters-->
          <Parameters>True:False:Exception|5|SingleNode1-Result|500</Parameters>
          <Trigger>
            <PhysicalPath>TimerTrigger.dll</PhysicalPath>
            <Parameters>S:4|S:5</Parameters>
          </Trigger>
        </Checker>
      </Checkers>
    </SubJob>
    <SubJob>
      <LogicalName>SubJob2</LogicalName>
      <LogicalExpression><![CDATA[SubJob1 & SubJob1Value1 & SubJob1Value2]]></LogicalExpression>
      <Checkers type="Array">
        <Checker>
          <LogicalName>SubJob1Value1</LogicalName>
          <PhysicalPath>TriggerEventMirrorChecker.dll</PhysicalPath>
          <Parameters>IsMirror</Parameters>
          <Trigger>
            <LogicalName>FirstNamedTreeEventTrigger</LogicalName>
            <Reference>LogicalResultChanged|Exception</Reference>
            <Parameters>SubJob1</Parameters>
          </Trigger>
          <!--Logger>
            <Reference>FirstNamedLogger</Reference>
          </Logger-->
        </Checker>
        <Checker>
          <LogicalName>SubJob1Value2</LogicalName>
          <PhysicalPath>TriggerEventMirrorChecker.dll</PhysicalPath>
          <Parameters>IsMirror</Parameters>
          <Trigger>
            <LogicalName>FirstNamedTreeEventTrigger</LogicalName>
            <Reference>LogicalResultChanged|Exception</Reference>
            <Parameters>SubJob1</Parameters>
          </Trigger>
          <!--Logger>
            <Reference>FirstNamedLogger</Reference>
          </Logger-->
        </Checker>
      </Checkers>
    </SubJob>
  </SubJobs>
</JobDescription>
