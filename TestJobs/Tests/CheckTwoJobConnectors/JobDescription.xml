<?xml version="1.0" encoding="utf-8"?>
<JobDescription>
  <LogicalName>JobConnectorTest</LogicalName>
  <LogicalExpression>
    <![CDATA[SubJob1 AND SubJob2]]>
  </LogicalExpression>
  <SubJobs type="Array">
    <SubJob>
      <LogicalName>SubJob1</LogicalName>
      <LogicalExpression><![CDATA[IS Checker]]></LogicalExpression>
      <Checkers type="Array">
		    <Checker>
		      <LogicalName>Checker</LogicalName>
		      <PhysicalPath>TrueFalseExceptionChecker.dll</PhysicalPath>
		      <Parameters>False:True|2</Parameters>
		      <Trigger>
		        <PhysicalPath>TimerTrigger.dll</PhysicalPath>
		        <Parameters>S:2</Parameters>
		      </Trigger>
		    </Checker>
      </Checkers>
    </SubJob>
    <SubJob>
      <LogicalName>SubJob2</LogicalName>
      <LogicalExpression><![CDATA[:SubJob1]]></LogicalExpression>
    </SubJob>
  </SubJobs>
</JobDescription>
