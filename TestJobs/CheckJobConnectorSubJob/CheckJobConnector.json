{
  "JobDescription": {
    "LogicalName": "CheckJobConnector",
    "LogicalExpression": "SubJob1 AND SubJob2",
    "SubJobs": {
      "SubJob": [
        {
          "LogicalName": "SubJob1",
          "LogicalExpression": "IS Checker",
          "Checkers": {
            "Checker": [
              {
                "LogicalName": "Checker",
                "PhysicalPath": "TrueFalseExceptionChecker.dll",
                "Parameters": "True:False:Exception|2",
                "Trigger": {
                  "PhysicalPath": "TimerTrigger.dll",
                  "Parameters": "S:2"
                }
              }
            ]
          }
        },
        {
          "LogicalName": "SubJob2",
          "LogicalExpression": ":SubJob1Value",
          "Checkers": {
            "Checker": [
              {
                "LogicalName": "SubJob1Value",
                "PhysicalPath": "TriggerEventMirrorChecker.dll",
                "Parameters": "IsMirror",
                "Trigger": {
                  "LogicalName": "FirstNamedTreeEventTrigger",
                  "Reference": "LogicalResultChanged|Exception",
                  "Parameters": "SubJob1"
                }
              }
            ]
          }
        }
      ]
    },
    "Workers": {
      "Worker": [
        {
          "LogicalExpression": "SubJob1:LogicalResultChanged|Exception",
          "SubWorkers": {
            "SubWorker": [
              {
                "PhysicalPath": "VishnuMessageBox.exe",
                "Parameters": "-Message=\"%Timestamp%: %Event% von %Source% in %Sender%#%Exception%\" -Caption=\"Server Event\" -MessageNewLine=#"
              }
            ]
          }
        },
        {
          "LogicalExpression": "CheckJobConnector:LogicalResultChanged|Exception",
          "SubWorkers": {
            "SubWorker": [
              {
                "PhysicalPath": "VishnuMessageBox.exe",
                "Parameters": "-Message=\"%Timestamp%: %Event% von %Source% in %Sender%#%Exception%\" -Caption=\"Server Event\" -MessageNewLine=#"
              }
            ]
          }
        }
      ]
    }
  }
}